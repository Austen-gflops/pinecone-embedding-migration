# Pinecone Vector Embedding Migration - Environment Variables
# Copy this file to .env and fill in your actual API keys

# =============================================================================
# PINECONE CONFIGURATION
# =============================================================================
# Get your API key from: https://app.pinecone.io/
PINECONE_API_KEY=your-pinecone-api-key-here

# Source index (contains the original OpenAI embeddings)
PINECONE_OLD_INDEX=gflops-serverless

# Target index (where Gemini embeddings will be stored)
PINECONE_NEW_INDEX=askdona

# Pinecone environment
PINECONE_ENVIRONMENT=us-west-2

# =============================================================================
# GOOGLE AI (GEMINI) CONFIGURATION
# =============================================================================
# Get your API key from: https://makersuite.google.com/app/apikey
GOOGLE_AI_API_KEY=your-google-ai-api-key-here

# Gemini embedding model (supports 3072 dimensions)
GEMINI_EMBEDDING_MODEL=gemini-embedding-001

# Output dimensions for embeddings
GEMINI_OUTPUT_DIMENSIONS=3072

# =============================================================================
# MIGRATION SETTINGS
# =============================================================================
# Default namespace to migrate
DEFAULT_NAMESPACE=your-namespace-uuid-here

# Batch sizes for processing
QUERY_BATCH_SIZE=100
EMBEDDING_BATCH_SIZE=100
UPSERT_BATCH_SIZE=100

# Delay between batches (seconds) to avoid rate limits
BATCH_DELAY=0.5

# =============================================================================
# NEW METADATA
# =============================================================================
# New metadata key-value pair to add to all migrated embeddings
NEW_METADATA_KEY=clearance_level
# IMPORTANT: This value is stored as an INTEGER (not string)
# e.g., clearance_level: 1 (not "1")
NEW_METADATA_VALUE=1
